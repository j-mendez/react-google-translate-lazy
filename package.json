{
  "name": "react-google-translate-lazy",
  "version": "1.0.0",
  "description": "lazy react google translate",
  "main": "./dist/index.js",
  "types": "types/index.d.ts",
  "author": "j-mendez",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "tsc && NODE_ENV=production postcss src/css/tailwind.css -o dist/css/tailwind.css",
    "postbuild": "npm run prepdist",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook -s public",
    "prepdist": "node ./config/prep-dist.js",
    "predeploy": "npm run build",
    "deploy": "cd dist && npm publish",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.7",
    "@types/jest": "^25.2.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.0.1",
    "autoprefixer": "9.8.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "css-loader": "^3.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "node-sass": "^4.13.1",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "styled-components": "^5.0.1"
  },
  "dependencies": {
    "@types/react-test-renderer": "^17.0.1",
    "styled-components": "^5.1.0",
    "tailwindcss": "1.3.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/tailwind/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
